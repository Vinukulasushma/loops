#include<stdio.h>
int main()
{
   int choice=0;
   printf("select a food item:1omlet price 30\n,2boiled egg price 20\n,3water bottle price 30\n,4pasta price 80\n,5chicken biriyani price 150\n");
   scanf("%d",&choice);
   switch(choice)
   {
          case 1:
                 printf(" choice is omlet price 30\n");
                 break;
          case 2:
                 printf("choice is boiled egg price 20\n");
                 break;
          case 3:
                 printf("choice is water bottle price 30\n");
                 break;
          case 4:
                 printf("choice is pasta price 80\n");
                 break;
          case 5:
                 printf("choice is chicken biriyani price 150\n");
                 break;
     }
     return 0;
 }
